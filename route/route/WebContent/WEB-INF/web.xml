  <servlet-mapping>
    <servlet-name>context</servlet-name>
    <url-pattern>/router</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>content</servlet-name>
    <servlet-class>servlet1.content</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>content</servlet-name>
    <url-pattern>/router1</url-pattern>
  </servlet-mapping>
